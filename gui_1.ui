            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button12">
            <property name="text" translatable="yes">button1</property>
            <property name="width">13</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button13">
            <property name="text" translatable="yes">button1</property>
            <property name="width">13</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button14">
            <property name="text" translatable="yes">button1</property>
            <property name="width">13</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button15">
            <property name="text" translatable="yes">button1</property>
            <property name="width">13</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Toplevel" id="toplevel5">
    <property name="container">false</property>
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="frame5">
